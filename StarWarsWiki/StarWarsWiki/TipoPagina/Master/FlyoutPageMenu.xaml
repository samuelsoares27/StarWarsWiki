<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StarWarsWiki.TipoPagina.Master.FlyoutPageMenu"
             xmlns:pages="clr-namespace:StarWarsWiki.TipoPagina.Master"
             xmlns:navegation="clr-namespace:StarWarsWiki.TipoPagina.Navegacao">
  <FlyoutPage.Flyout>
    <pages:FlyoutPageMenuFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
     <x:Arguments>
        <navegation:Inicio />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>